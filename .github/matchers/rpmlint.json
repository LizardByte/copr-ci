{
  "problemMatcher": [
    {
      "owner": "rpmlint-generic",
      "pattern": [
        {
          "regexp": "^(error|warning): ([^ ]+) line (\\d+): (.*)$",
          "severity": 1,
          "file": 2,
          "line": 3,
          "message": 4
        }
      ]
    },
    {
      "owner": "rpmlint-warning",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^([^:]+):\\sW:\\s(.*)$",
          "file": 1,
          "message": 2
        }
      ]
    },
    {
      "owner": "rpmlint-error",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^([^:]+):\\sE:\\s(.*)$",
          "file": 1,
          "message": 2
        }
      ]
    }
  ]
}
